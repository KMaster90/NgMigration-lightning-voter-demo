{
  "name": "lightning-voter-demo",
  "version": "1.0.0",
  "description": "lightning talk voting app",
  "main": "index.js",
  "scripts": {
    "server": "node ./server/server.js",
    "dev": "nodemon ./server/server.js -w ./server",
    "start": "npm run clean-dev && webpack --config config/webpack.dev.js --watch --profile --progress",
    "buildJS": "npm run clean-aot && webpack --config config/webpack.ng1.prod.js --profile --progress --bail",
    "buildA": "npm run clean-aot && webpack --config config/webpack.prod.js --profile --progress --bail",
    "build": "npm run buildJS && npm run buildA",
    "clean-dev": "rimraf dist/dev/*",
    "clean-aot": "rimraf dist/aot/*",
    "testjs": "karma start config/angularjs-karma.conf.js",
    "test": "karma start config/angular-karma.conf.js",
    "tsc": "tsc -p . -w"
  },
